Vec2 :: (u32, u32);
Option :: [Some u32, None ()];

unwrap: Option -> u32 = x -> match x {
  (x, x) -> x,
    Some[x] -> x,
    None[()] -> unreachable
};

add: (u32, u32) -> u32 = (a, a) -> a;
add: (Vec2, Vec2) -> Vec2 = (a, b) -> (a.0.add(b.0), a.1.add(b.1));


map: (Option, u32 -> u32) -> Option = (x, f) -> match x {
  Some[x] -> Some[f(x)],
  None[()] -> None[()]
};
